{
    "name": "Image B Gone",
    "version": "0.1.0",
    "description": "Remove all images on a webpage",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_icon": {
        "16": "assets/ext-icon.png",
        "24": "assets/ext-icon.png",
        "32": "assets/ext-icon.png"
      },
      "default_title": "Image B Gone",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}